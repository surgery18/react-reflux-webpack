{
  "name": "webpack-test",
  "version": "1.0.0",
  "description": "testing webpack builder",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch --onlyChange",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^15.3.0-rc.3",
    "react-dom": "*",
    "reflux": "^0.4.1"
  },
  "devDependencies": {
    "babel-jest": "^13.2.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2017": "^1.4.0",
    "babel-preset-react": "^6.11.1",
    "enzyme": "^2.4.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "jest-cli": "*"
  },
  "jest": {
    "testDirectoryName": "src/unit_tests",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "reflux",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "underscore",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys"
    ]
  }
}
